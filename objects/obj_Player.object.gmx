        <arguments>
          <argument>
            <kind>1</kind>
            <string>/*PLAYER MOVEMENT*/

if (keyboard_check(ord("W")) ){
    if(rotate&amp;&amp;obj_Controller.game_state=="running"){
    looking="up";
    image_angle=0;
    }
    else{
    looking="up";
    image_angle=0;
    if(y&gt;32&amp;&amp;!(place_meeting(x,y-1,obj_Obstacle0)||place_meeting(x,y-1,obj_Obstacle1)||place_meeting(x,y-1,obj_Obstacle2))&amp;&amp;
    obj_Controller.game_state=="running")y=y-1;}
}

if (keyboard_check(ord("S")) ){
  if(rotate&amp;&amp;obj_Controller.game_state=="running"){
    looking="down";
    image_angle=180;
    }
    else{
    looking="down";
    image_angle=180;
    if(y&lt;room_height-32&amp;&amp;!(place_meeting(x,y+1,obj_Obstacle0)||place_meeting(x,y+1,obj_Obstacle1)||place_meeting(x,y+1,obj_Obstacle2))
    &amp;&amp;obj_Controller.game_state=="running")y=y+1}
}

if (keyboard_check(ord("A")) ){
    if(rotate&amp;&amp;obj_Controller.game_state=="running"){
    looking="left";
    image_angle=90;
    }
    else{
    looking="left";
    image_angle=90;
    if(x&gt;32&amp;&amp;!(place_meeting(x-1,y,obj_Obstacle0)||place_meeting(x-1,y,obj_Obstacle1)||place_meeting(x-1,y,obj_Obstacle2))
    &amp;&amp;obj_Controller.game_state=="running")x=x-1}
}

if (keyboard_check(ord("D")) ){
 if(rotate&amp;&amp;obj_Controller.game_state=="running"){
    looking="right";
    image_angle=270;
    }
    else{
    looking="right";
    image_angle=270;
    if(!(place_meeting(x+1,y,obj_Obstacle0)||place_meeting(x+1,y,obj_Obstacle1)||place_meeting(x+1,y,obj_Obstacle2))
    &amp;&amp;x&lt;room_width-32&amp;&amp;obj_Controller.game_state=="running")x=x+1} 
    }

/*BEEPING CONTROL*/
    
    
distance_to_mine=distance_to_object(instance_nearest(x,y,obj_Mine));    //propably the most important value in whole code, so leave it as it is!!11one


//mine is detected
if(distance_to_mine&lt;=60&amp;&amp;mine_detected==false&amp;&amp;!mine_danger){
    alarm[0]=1; 
    mine_detected=true;
}

//mine is close to player
if(distance_to_mine&lt;=20&amp;&amp;mine_danger=false){
    alarm[2]=1;
    mine_danger=true;
}

//mine is no longer close to player, but is still detected
else if(distance_to_mine&gt;20&amp;&amp;mine_danger==true){
    mine_danger=false;
    alarm[0]=1;
    mine_detected=true;
}

//player is away from mines
else if(distance_to_mine&gt;60){
    alarm[1]=1; 
    mine_danger=false;  
    mine_detected=false;
}

//Make player sprite invisible after game over
if obj_Controller.lifes==0 image_alpha=0;
  

</string>
          </argument>
